actor Act <3>
    receiver init()
        write("init at Act")
        self << koro()
        self << R()
    end

    receiver R()
        write("R at Act")
    end

    receiver koro()
        write("koro at Acira")
    end
end

actor Acira<3>
    receiver koro()
        write("koro at Acira")
    end
end